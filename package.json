{
  "name": "setup",
  "version": "2.0.0",
  "main": "main.js",
  "license": "MIT",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "build": "tsup src/",
    "build:watch": "tsup src/ --watch",
    "start": "NODE_ENV='prod' node dist/main.js",
    "start:dev": "tsx watch --clear-screen=false src/main.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitest/ui": "^0.32.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "^4.8.4",
    "vitest": "^0.31.4"
  }
}
